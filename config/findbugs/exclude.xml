<FindBugsFilter>
    <Match>
        <Class name="~.*_\$.*"/>
    </Match>
    <Match>
       <Class name="~.*R\$.*"/>
    </Match>
    <Match>
        <Class name="~.*Manifest\$.*"/>
    </Match>
